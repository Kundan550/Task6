    "from datetime import datetime\n",
    "import pywhatkit\n",
    "now = datetime.now()\n",
    "\n",
    "time = now.strftime(\"%m %M\")\n",
    "hours = now.strftime(\"%d %H\")\n",
    "\n",
    "\n",
    "hour=int(hours[3:5]) \n",
    "minute=int(time[3:6]) + 2\n",
    "\n",
    "pywhatkit.sendwhatmsg('+919926436393','face detected..!',hour,minute)"
   ]
  }
